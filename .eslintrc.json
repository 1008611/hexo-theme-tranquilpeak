{
    "extends": "google",
    "rules": {
        "comma-dangle": [2,"never"],
        "valid-jsdoc": [2, {
            "requireReturnDescription": false,
            "requireParamDescription": false
        }],
        "no-undef":[0],
        "brace-style": [2, "stroustrup"],
        "no-trailing-spaces": [2, {"skipBlankLines": true }],
        "eqeqeq":[1],
        "max-len": [1, 100, 4, {"ignoreUrls": true}]
    },
    "env": {
        "browser": true,
        "node": true
    }
}